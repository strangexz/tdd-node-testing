{
  "name": "tdd-node-testing",
  "version": "1.0.0",
  "description": "Plantilla para proyectos API en Node JS",
  "main": "index.js",
  "scripts": {
    "local": "nodemon",
    "test": "jest --detectOpenHandles --forceExit",
    "db:migrate": "npx knex -- migrate:latest && npx knex migrate:latest --env test",
    "db:rollback": "npx knex -- migrate:rollback && npx knex migrate:rollback --env test",
    "db:reset": "npm run db:rollback && npm run db:setup",
    "db:seed": "npx knex -- seed:run && npx knex seed:run --env test",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strangexz/node-project-template.git"
  },
  "author": "strangexz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/strangexz/node-project-template/issues"
  },
  "homepage": "https://github.com/strangexz/node-project-template#readme",
  "dependencies": {
    "@faker-js/faker": "^7.4.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-http-context": "^1.2.4",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "objection": "^3.0.1",
    "sqlite3": "^5.0.11",
    "underscore": "^1.13.4",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1",
    "yargs": "^17.5.1"
  },
